{
  "name": "fbn-group",
  "version": "1.0.0",
  "description": "fbn group frontend & backend",
  "main": "server.js",
  "proxy": "https://fbn-group.com",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "git": "git add -A && git commit -m 'Oppdatert' && git push origin master"
  },
